{
  "name": "nomoney",
  "outputDirectory": ".",
  "buildCommand": "",
  "installCommand": "",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600, must-revalidate" }
      ]
    },
    {
      "source": "/(.*\\.(png|svg|jpg|jpeg|gif|ico|css|js))",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
